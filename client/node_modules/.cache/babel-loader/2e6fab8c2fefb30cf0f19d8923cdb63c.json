A,EAAA,MAAA;AACA;;AACA,aAAA,KAAA;AACA,KAdA;AAeA,IAAA,WAfA,yBAeA;AAAA;;AACA,WAAA,OAAA,GAAA,IAAA;AACA,UAAA,IAAA,8CAAA,KAAA,UAAA,qBAAA,KAAA,UAAA,oBAAA,KAAA,SAAA,CAAA;AACA,WAAA,SAAA,GAAA,MAAA,CAAA,YAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA,MAAA,KAAA,CACA,GADA,CACA,IADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,YAAA,OAAA,GAAA,KAAA,CAAA,YAAA,EAAA;;AACA,QAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,OAAA,EAAA;AACA,cAAA,GAAA,GAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,CAAA,MAAA,CAAA,YAAA,CAAA;AACA,cAAA,GAAA,GAAA,OAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,mBAAA,CAAA,CAAA,IAAA,KAAA,GAAA;AAAA,WAAA,EAAA,CAAA,CAAA;;AACA,cAAA,OAAA,GAAA,KAAA,WAAA,IAAA,GAAA,KAAA,IAAA,EAAA;AACA,YAAA,GAAA,CAAA,KAAA,GAAA,OAAA,CAAA,KAAA;AACA,YAAA,GAAA,CAAA,QAAA,GAAA,OAAA,CAAA,QAAA;AACA;AACA,SAPA;AAQA,QAAA,OAAA,CAAA,UAAA,GAAA,KAAA,CAAA,UAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,OAAA;AACA,OAdA,EAeA;AAfA,OAgBA,OAhBA,CAgBA,KAAA,OAAA,GAAA,KAhBA;AAiBA,KApCA;AAqCA,IAAA,oBArCA,kCAqCA;AAAA;;AACA,UAAA,IAAA,GAAA,4CAAA;AACA,MAAA,KAAA,CACA,GADA,CACA,IADA,EAEA,IAFA,CAEA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,WAAA,GAAA,QAAA,CAAA,IAAA;AACA,OAJA,EAFA,CAOA;AACA;AA7CA,GAnBA;AAkEA,EAAA,OAlEA,qBAkEA;AACA,SAAA,WAAA;AACA,SAAA,oBAAA;AACA;AArEA,CAAA","sourcesContent":["<template>\n  <body>\n      <Navigation></Navigation>\n      <div id=\"app\">\n        <form action=\"#\" @submit.prevent=\"getArticles\">\n          <div class=\"form-group\">\n            <input\n              type=\"text\"\n              placeholder=\"Gib hier einen Suchbegriff fÃ¼r den Artikelinhalt ein!\"\n              v-model=\"searchterm\"\n              class=\"\"\n            ><br/>\n            <!--<input\n              type=\"text\"\n              placeholder=\"Gib hier einen Autor ein!\"\n              v-model=\"authorterm\"\n              class=\"col-md-2 col-md-offset-5\"\n            ><br/>\n            <input\n              type=\"text\"\n              placeholder=\"Gib hier einen Titel ein!\"\n              v-model=\"titleterm\"\n              class=\"col-md-2 col-md-offset-5\"\n            >-->\n          </div>\n        </form>\n        <div class=\"alert alert-info\" v-show=\"loading\">Lade...</div>\n        <div class=\"alert alert-danger\" v-show=\"errored\">Es trat ein Fehler auf.</div>\n        <amchartstimeserieschart :articlesattribute=\"articlesnew\"></amchartstimeserieschart>\n        <amchartsweekdayhour :articlesattribute=\"weekdayhour\"></amchartsweekdayhour>\n      </div>\n  </body>\n</template>\n\n<script>\n\nimport axios from 'axios';\nimport moment from 'moment';\nimport amchartsweekdayhour from '@/components/amchartsweekdayhour.vue';\nimport amchartstimeserieschart from '@/components/amchartstimeserieschart.vue';\nimport Navigation from '@/components/navigation.vue';\n\nexport default {\n  name: 'app',\n  components: {\n    amchartstimeserieschart,\n    amchartsweekdayhour,\n    Navigation,\n  },\n  data() {\n    return {\n      loading: false,\n      errored: false,\n      articlesnew: [],\n      weekdayhour: [],\n      searchterm: '',\n      authorterm: '',\n      titleterm: '',\n      startDate: null,\n    };\n  },\n  methods: {\n    getDateRange() {\n      const startDate = moment('2020-02-08');\n      const endDate = moment();\n      const dates = [];\n      while (startDate.isSameOrBefore(endDate)) {\n        dates.push({\n          date: startDate.format('YYYY-MM-DD'),\n          count: 0,\n          countdpa: 0,\n        });\n        startDate.add(1, 'days');\n      }\n      return dates;\n    },\n    getArticles() {\n      this.loading = true;\n      const path = `http://localhost:5000/articles?s=${this.searchterm}&author=${this.authorterm}&title=${this.titleterm}`;\n      this.startDate = moment('2020-02-08').format('YYYY-MM-DD');\n      axios\n        .get(path)\n        .then((response) => {\n          const payload = this.getDateRange();\n          response.data.forEach((element) => {\n            const key = moment(element.date).format('YYYY-MM-DD');\n            const obj = payload.filter((o) => o.date === key)[0];\n            if ((typeof obj !== 'undefined') && (obj !== null)) {\n              obj.count = element.count;\n              obj.countdpa = element.countdpa;\n            }\n          });\n          payload.searchterm = this.searchterm;\n          this.articlesnew = payload;\n        })\n        // .catch((error) => { console.error(error); });\n        .finally(this.loading = false);\n    },\n    getWeekdayHourLength() {\n      const path = 'http://localhost:5000/articles/hour/length';\n      axios\n        .get(path)\n        .then((response) => {\n          this.weekdayhour = response.data;\n        });\n    // .catch((error) => { console.error(error); });\n    },\n  },\n  mounted() {\n    this.getArticles();\n    this.getWeekdayHourLength();\n  },\n};\n</script>\n<!--@import '../public/new.min.css';-->\n<style>\n@import url('https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap');\n@import url('https://fonts.googleapis.com/css?family=Sanchez&display=swap');\n@import url('https://fonts.googleapis.com/css?family=Arimo&display=swap');\nbody {\n  display:block;\n  max-width: 800px;\n  margin-left: auto;\n  margin-right: auto;\n  margin-top:60px;\n  color: #2f2f2f;\n  font-family: \"Roboto\";\n  font-size: 20px;\n  line-height: 1.7;\n  overflow-wrap: break-word;\n}\n#app {\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\ninput {\n  width:100%;\n  text-align:center;\n}\n.bar {\n  fill: #626d79;\n  }\n</style>\n\n/* eslint-disable */\n"],"sourceRoot":"src/views"}]}